{
  "name": "@ifrs15/monorepo",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --target=serve --projects=web,api --parallel",
    "dev:web": "nx serve web",
    "dev:api": "nx serve api",
    "build": "nx run-many --target=build --all",
    "build:web": "nx build web",
    "build:api": "nx build api",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "nx run infra:db:generate",
    "db:push": "nx run infra:db:push",
    "db:migrate": "nx run infra:db:migrate",
    "db:migrate:dev": "nx run infra:db:migrate:dev",
    "db:migrate:reset": "nx run infra:db:migrate:reset",
    "db:seed": "nx run infra:db:seed",
    "db:seed:dev": "nx run infra:db:seed:dev",
    "db:studio": "nx run infra:db:studio",
    "db:reset": "nx run infra:db:reset",
    "keycloak:import": "docker-compose exec keycloak /opt/keycloak/bin/kc.sh import --file /opt/keycloak/data/import/realm-export.json",
    "prepare": "husky install",
    "postinstall": "nx run-many --target=postinstall --all",
    "clean": "nx reset && rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "clean:build": "nx run-many --target=clean --all",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "graph": "nx graph",
    "dep-graph": "nx dep-graph",
    "release": "nx run-many --target=build --all --configuration=production && nx run-many --target=test --all",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "typecheck": "nx run-many --target=typecheck --all"
  },
  "devDependencies": {
